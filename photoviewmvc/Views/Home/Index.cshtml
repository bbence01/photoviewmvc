@model List<string>
<div class="container">
    <div class="row">
        @foreach (var image in Model)
        {
            <div class="col-md-4">
                <div class="card">
                    <img class="card-img-top" src="@Url.Content("~/Pictures/" + image)" alt="Image">
                </div>
            </div>
        }
    </div>
</div>
